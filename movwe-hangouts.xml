<?xml version="1.0" encoding="UTF-8" ?>
<Module>
    <ModulePrefs title="Hangout app">
        <Require feature="rpc"/>
        <Require feature="views"/>
    </ModulePrefs>
    <Content type="html">
        <![CDATA[
      <!doctype html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link href="https://test-movwe.googlecode.com/git/lib/bower/videojs/dist/video-js/video-js.css" rel="stylesheet"/>
    <link href="https://test-movwe.googlecode.com/git/lib/bower/videojs-sublime-skin/dist/videojs-sublime-skin.css" rel="stylesheet"/>
    <link href="https://test-movwe.googlecode.com/git/lib/bower/bootswatch-dist/css/bootstrap.css" rel="stylesheet"/>
    <link href="https://test-movwe.googlecode.com/git/lib/bower/fontawesome/css/font-awesome.css" rel="stylesheet">
    <link href="index.css" rel="stylesheet">
    <script src="//plus.google.com/hangouts/_/api/v1/hangout.js"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/videojs/dist/video-js/video.dev.js" type="text/javascript"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/socket.io-client/socket.io.js"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/jquery/dist/jquery.js"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/moment/moment.js"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/underscore/underscore.js"></script>
    <script src="https://test-movwe.googlecode.com/git/lib/bower/bootswatch-dist/js/bootstrap.js" type="text/javascript"></script>
    <script src="https://test-movwe.googlecode.com/git/index.js" type="text/javascript"></script>
    <script>
        socket = io('https://blah');
    </script>
</head>
<body>
<div class="container">
    <div class="page-header">
        <h1>MovWe <small>powered by video.js and socket.io</small></h1>
    </div>
    <div class="video">
        <video id="main_video" class="video-js vjs-sublime-skin"
               controls preload="auto" width="640" height="360"
               data-setup='{"example_option":true}'>
            <p class="vjs-no-js">To view this video please enable JavaScript, and consider upgrading to a web browser
                that <a href="http://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a></p>
        </video>
    </div>

    <div id="groupControls">
        <button id="play" class="btn btn-success btn-lg" disabled><i class="fa fa-play"></i> Play Everyone</button>
        <button id="pause" class="btn btn-warning btn-lg" disabled><i class="fa fa-pause"></i> Pause Everyone</button>
    </div>

    <div id="controls" class="well bs-component">
        <form id="addressForm" class="form-inline">
            <fieldset id="media">
                <legend>Media</legend>
                <label for="address">Address</label> <input id="address" class="form-control" type="text" name="address" placeholder="0.0.0.0"/>
                <label for="port">Port</label> <input id="port" class="form-control" type="number" name="port" min="0" max="65535" value="32400"/>
                <label for="metadata">Metadata ID</label> <input id="metadata" class="form-control" type="number" name="metadata" min="0" max="999999" placeholder="1234"/>
            </fieldset>
            <fieldset id="quality">
                <legend>Quality</legend>
                <input id="720p" class="form-control" type="radio" name="resolution" value="1280x720" /> <label for="720p">720p (Good Internet)</label>
                <input id="360p" class="form-control" type="radio" name="resolution" value="640x360" checked /> <label for="360p">360p (OK Internet)</label>
                <input id="180p" class="form-control" type="radio" name="resolution" value="320x180" /> <label for="180p">180p (Sad Internet)</label>
            </fieldset>
            <fieldset id="time">
                <legend>Start Time</legend>
                <input id="hours" class="form-control" type="number" min="0" max="23" placeholder="0" /> hrs.
                <input id="minutes" class="form-control" type="number" min="0" max="59" placeholder="0" /> mins.
                <input id="seconds" class="form-control" type="number" min="0" max="59" placeholder="0" /> secs.
            </fieldset>
            <button id="load" type="submit" class="btn btn-success">Load</button>
        </form>
    </div>

    <div class="footer">
        <p class="pull-right"><i class="fa fa-heart"></i> from MovWe</p>
    </div>
</div>

</body>
</html>

    ]]>
    </Content>
</Module>